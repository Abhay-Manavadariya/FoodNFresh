{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js",
      "methods": ["GET", "POST", "PATCH", "DELETE", "OPTIONS"]
    }
  ],
  "functions": {
    "index.js": {
      "memory": 3009,
      "maxDuration": 30
    },
    "*.js": {
      "memory": 3009,
      "maxDuration": 30
    }
  }
}
